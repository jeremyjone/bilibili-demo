page{
  color: #666;
  padding: 10rpx;
}

/* 首页导航 */
.nav_warp{}
.nav{
  white-space: nowrap;
  padding: 5rpx 0;
}

.nav_item{
  padding: 20rpx 45rpx;
  font-size: 30rpx;
  display: inline-block;
}

.nav_item.active{
  border-bottom: 5rpx solid pink;
}

/* 轮播图 */
.slides{
  margin-top: 10rpx;
}

.slides swiper{
  height: 220rpx;
}

.slides nagivator{
  width: 100%;
  height: 100%;
}

.slides image{
  width: 100%;
  height: 100%;
}

/* 视频列表 */
.video_wrap{
  display: flex;
  flex-wrap: wrap;
  padding: 5rpx;
  justify-content: space-between;
}

.video_item{
  width:48%;
  margin-bottom: 20rpx;
}

.video_img{
  position: relative;
}

.video_img image{
  width: 100%;
}

.video_img .video_info{
  width: 100%;
  position: absolute;
  left: 0;
  bottom: 10rpx;
  display: flex;
  justify-content: space-around;
}

.video_img .video_info .count_wrap{
  display: flex;
  color: #fff;
}

.video_title{
  font-size: 28rpx;
  display: -webkit-box;
  white-space: normal;
  text-overflow: ellipsis;
  word-wrap: break-word;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
}

.icon_style{
  width: 40rpx;
  height: 40rpx;
}

.play_icon{
  background-image: url("data:image/svg+xml;base64,");
  background-size: cover;
}

.comment_icon{
  background-image: url("data:image/svg+xml;base64,");
  background-size: cover;
}